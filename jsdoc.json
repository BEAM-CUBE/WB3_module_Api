{
  "source": {
    "include": ["src/"],
    "exclude": ["node_modules/"],
    "includePattern": ".js$",
    "excludePattern": "(node_modules/|docs/|public/|.vscode/|dist)"
  },
  "plugins": ["plugins/markdown"],
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": true
  },
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc", "closure"]
  },
  "opts": {
    "recurse": true,
    "encoding": "utf8",
    "destination": "./docs/"
  },
  "recurseDepth": 10,
  "verbose": true
}
